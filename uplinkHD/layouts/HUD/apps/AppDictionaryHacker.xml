<layout name="AppDictionaryHacker" w="90" h="90">
  <Image name="base" file="appBase_01.png" x="0" y="0" alpha="1"/>
  <ButtonStandard name="btnStart" x="4" y="4" w="82" h="82" scalingType="scaleNormal" arrangeType="arrangeFree" arrangePadding="0" alpha="1">
    <Image name="gfxHover" file="appBtn_01_h.png" x="0" y="0" alpha="1"/>
    <Image name="gfxClick" file="appBtn_01_c.png" x="0" y="0" alpha="1"/>
  </ButtonStandard>
  <Image name="icon" file="appIcon_dictionaryHacker.png" x="16" y="11" alpha="1"/>
  <TextPoint name="txtWord" x="45" y="36" align="center" font="AeroMaticsLightRegular" size="18" text="Longword" fillColor="FFFFFF" alpha="1"/>
  <Image name="iconError" file="appIcon_error_01.png" x="22" y="22" alpha="1"/>
  <Image name="iconDone" file="appIcon_done_01.png" x="18" y="26" alpha="1"/>
  <ButtonStandard name="btnClose" x="72" y="0" w="18" h="18" scalingType="scaleNormal" arrangeType="arrangeFree" arrangePadding="0" alpha="1">
    <Image name="gfxStandard" file="appBtn_close_s.png" x="0" y="0" alpha="1"/>
    <Image name="gfxHover" file="appBtn_close_h.png" x="0" y="0" alpha="1"/>
    <Image name="gfxClick" file="appBtn_close_c.png" x="0" y="0" alpha="1"/>
  </ButtonStandard>
</layout>